 <!-- form handling -->
 <form action="/api/users" method="POST" id="add_user">
    <div class="new_user">
        <div class="form-group">
            <label for="name" class="text-light">Participant</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="name" value="" placeholder="Chennai Super Kings">
        </div>
        <div class="form-group">
            <label for="Email" class="text-light">Venue</label>
            <input type="text" name="email" value="" placeholder="Chennai">
        </div>
        <div class="form-group">
            <label for="gender" class="text-light">Dates</label>
            <!-- <div class="radio inline">
                <input type="radio" id="radio-2" name="gender" value="Male" >
                <label for="radio-2" class="radio-label">Male</label>
            </div> -->
            <div class="radio inline">
            <input type="date" id="datepicker" name='from' size='9' value="Start" /> 
            <label for="radio-2" class="radio-label">Start</label>
        </div>
        <div class="radio inline">
            <input type="date" id="datepicker" name='from' size='9' value="End" /> 
            <label for="radio-2" class="radio-label">End</label>
        </div>
 
            <!-- <div class="radio inline">
                <input type="radio" id="radio-3" name="gender" value="Female" >
                <label for="radio-3" class="radio-label">Female</label>
            </div>
        </div> -->

        <div class="form-group">
            <label for="gender" class="text-light">Status</label>
            <div class="radio inline">
                <input type="radio" id="radio-4" name="status" value="Active" >
                <label for="radio-4" class="radio-label">WIN</label>
            </div>
            <div class="radio inline">
                <input type="radio" id="radio-5" name="status" value="Inactive" >
                <label for="radio-5" class="radio-label">LOSS</label>
            </div>
        </div>

        <div class="form-group">
            <button type="submit" class="btn text-dark update">Save</button>
        </div>

    </div>
</form>